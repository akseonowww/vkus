{
   "author": "Kwilz",
   "content_scripts": [ {
      "js": [ "assets/js/load.js" ],
      "matches": [ "https://vk.com/*", "https://m.vk.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Анимируйте ваш Вконтакте, настройте стиль анимаций с меню в профиле или настройках VK Styles. Поддерживает различные расширения.",
   "homepage_url": "https://vk.com/lolaka40",
   "host_permissions": [ "*://m.vk.com/*", "*://vk.com/*" ],
   "icons": {
      "128": "assets/icons/128.png",
      "512": "assets/icons/512.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "VK Animated - Анимации Вконтакте",
   "short_name": "VK Animated",
   "update_url": "https://clients2.google.com/service/update2/crx",
